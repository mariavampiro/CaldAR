{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["error", {"vars": "local", "args": "none"}],
    "camelcase": "off",
    "prefer-destructuring": "off",
    "consistent-return": "off",
    "no-console": "off"
  },
  "plugins": []
}
